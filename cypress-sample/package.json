{
  "name": "cypress-sample",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:head": "cypress run --headed --no-exit",
    "clean": "rimraf cypress/reports cypress/mochawesome.json mochawesome-report",
    "marge": "mochawesome-merge \"cypress/reports/*.json\" > ./cypress/mochawesome.json && npx marge ./cypress/mochawesome.json",
    "update": "npm run clean && sleep 1 && npm run cy:run && sleep 1 && npm run marge"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^13.13.1",
    "playwright-webkit": "1.34"
  },
  "dependencies": {
    "cypress-parallel": "^0.14.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1",
    "sleep": "^6.3.0"
  }
}
